{
    "name": "sgds-govtech",
    "version": "1.2.0",
    "description": "Design System unites Singapore Government digital services around a common visual language and user experience",
    "license": "MIT",
    "author": "Tey Liang Tai",
    "files": [
        "css",
        "fonts",
        "js"
    ],
    "main": "js/sgds.js",
    "scripts": {
        "build": "npm run packagedownloads && npm run buildsgds && bundle exec jekyll build",
        "buildsgds": "NODE_ENV=production webpack --mode=production",
        "devserver": "webpack --watch & bundle exec jekyll serve",
        "packagedownloads": "./scripts/packagedownloads.sh",
        "prepublishOnly": "npm run buildsgds",
        "start": "bundle exec jekyll serve",
        "test": "snyk test"
    },
    "dependencies": {
