{
  "name": "@govtechsg/sgds",
  "version": "2.0.0",
  "description": "Design System unites Singapore Government digital services around a common visual language and user experience",
  "repository": {
    "type": "git",
    "url": "https://github.com/govtechsg/sgds.git"
  },
  "license": "MIT",
  "author": "Tey Liang Tai",
  "main": "css/sgds.css",
  "files": [
    "sass",
    "css"
  ],
  "scripts": {
    "build": "npm run packagedownloads && npm run build:site && bundle exec jekyll build",
    "build:lib": "rm -rf ./css && webpack --mode=production",
    "build:site": "npm run build:lib",
    "dev": "concurrently npm:dev:static npm:dev:jekyll",
    "dev:jekyll": "bundle exec jekyll serve",
    "dev:static": "webpack --watch",
    "packagedownloads": "./build/packagedownloads.sh",
    "prepublishOnly": "npm run build:lib",
    "start": "bundle exec jekyll serve",
    "prepare": "husky install"
  },
  "devDependencies": {
    "@babel/core": "^7.11.6",
    "@babel/plugin-proposal-object-rest-spread": "^7.11.0",
    "@babel/preset-env": "^7.11.5",
    "@commitlint/cli": "^17.0.3",
    "@commitlint/config-conventional": "^17.0.3",
    "axios": "^0.21.1",
    "clipboard": "^2.0.10",
    "concurrently": "^7.0.0",
    "copy-webpack-plugin": "^10.2.4",
    "css-loader": "^6.7.1",
    "css-minimizer-webpack-plugin": "^3.4.1",
    "cz-conventional-changelog": "^3.3.0",
    "glob": "^7.2.0",
    "husky": "^8.0.1",
    "list.js": "^1.5.0",
    "lodash": "^4.17.21",
    "lunr": "^2.3.9",
    "mini-css-extract-plugin": "^2.6.0",
    "purgecss-webpack-plugin": "^4.1.3",
    "sanitize-html": "^2.3.3",
    "sass": "^1.49.9",
    "sass-loader": "^12.6.0",
    "v-tooltip": "^2.0.2",
    "vue": "^2.6.12",
    "vue-loader": "^15.9.6",
    "vue-loading-overlay": "^3.4.2",
    "vue-moment": "^4.1.0",
    "vue-template-compiler": "^2.6.12",
    "vuex": "^3.6.0",
    "webpack": "^5.70.0",
    "webpack-cli": "^4.9.2",
    "webpack-merge": "^5.8.0"
  },
  "dependencies": {
    "@govtechsg/sgds-masthead": "^0.0.9",
    "@popperjs/core": "^2.11.5",
    "@vue/composition-api": "^1.7.0",
    "bootstrap": "^5.1.3",
    "gsap": "^3.9.1",
    "shufflejs": "^6.0.0"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
